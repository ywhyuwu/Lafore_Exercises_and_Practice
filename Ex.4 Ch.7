#include <iostream>
#include <string>
#include <cstring>

using namespace std;
const int MAX = 5;
int maxint(int arr[MAX]);
int main()
{
	int integer[MAX];
	cout << "Enter an integers: ";
	for (int i = 0; i < MAX; i++)
	{
		cin >> integer[i];
	}
	int max;
	max = maxint(integer);
	cout << "Max integer is " << max << endl;
	cin.get();
	cin.get();
	cin.get();
	cin.get();
	cin.get();
	return 0;
}

int maxint(int arr[MAX])
{
	int max = 0;
	for (int j = 0; j < MAX; j++)
	{
		if (max < arr[j])
			max = arr[j];
	}
	return max;
}
